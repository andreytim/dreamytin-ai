{
  "name": "dreamytin-ai",
  "version": "1.0.0",
  "description": "Personal AI assistant with local and remote model support",
  "main": "src/main/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "concurrently \"npm run backend\" \"npm run electron:dev\"",
    "electron:dev": "nodemon --exec electron .",
    "backend": "nodemon src/backend/server.js",
    "build:renderer": "vite build",
    "dev:renderer": "vite"
  },
  "devDependencies": {
    "electron": "^28.0.0",
    "nodemon": "^3.0.0",
    "vite": "^5.0.0",
    "@vitejs/plugin-react": "^4.2.0",
    "concurrently": "^8.2.0"
  },
  "dependencies": {
    "ai": "^3.0.0",
    "@ai-sdk/openai": "^0.0.0",
    "@ai-sdk/anthropic": "^0.0.0",
    "express": "^4.18.0",
    "cors": "^2.8.0",
    "dotenv": "^16.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}